{
  "name": "humanized-object-diff",
  "description": "Using Readable Sentences to describe differences between two Plain Objects, and providing self-defined templates to organize your own sentences.",
  "version": "1.0.1",
  "author": "daxiong",
  "dependencies": {
    "date-fns": "^2.8.1",
    "deep-diff": "^1.0.2",
    "fast-deep-equal": "^3.1.1",
    "handlebars": "^4.7.6",
    "humanize-string": "^2.1.0",
    "lodash": "^4.17.15",
    "titleize": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-flow": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/runtime": "^7.7.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "ava": "^3.4.0",
    "babel-eslint": "^10.0.3",
    "codecov": "^3.6.5",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-xo-lass": "latest",
    "fixpack": "^3.0.5",
    "flow-bin": "^0.113.0",
    "gh-pages": "^2.1.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.11",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-image": "^1.0.2",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "^1.0.2",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^5.1.3",
    "serve": "^11.2.0",
    "standard-version": "^7.0.1",
    "xo": "^0.27.1",
    "react-dom": "^16.13.1",
    "react": "^16.13.1"
  },
  "engines": {
    "node": ">=8.3"
  },
  "homepage": "https://github.com/dxmz/humanized-object-diff",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "deep-diff",
    "human-readable",
    "humanize",
    "object-diff",
    "diff",
    "object difference",
    "js diff"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "xo --fix",
      "git add"
    ],
    "package.json": [
      "fixpack",
      "git add"
    ]
  },
  "main": "index.js",
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dxmz/humanized-object-diff"
  },
  "scripts": {
    "ava": "cross-env NODE_ENV=test ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "xo",
    "nyc": "cross-env NODE_ENV=test nyc ava",
    "test": "yarn run lint && yarn run ava",
    "test-coverage": "yarn run lint && yarn run nyc",
    "dev-build": "rollup -c scripts/rollup.config.dev.js",
    "prepublishOnly": "npm run build",
    "clean-dist": "rimraf example/dist",
    "clean-lib": "rimraf lib",
    "start": "npm run clean-dist && cross-env NODE_ENV=development rollup -w -c scripts/rollup.config.dev.js",
    "build": "npm run clean-lib && cross-env NODE_ENV=production rollup -c scripts/rollup.config.prod.js",
    "deploy": "gh-pages -d example/dist",
    "publish-demo": "npm run dev-build && npm run deploy"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ]
  }
}
